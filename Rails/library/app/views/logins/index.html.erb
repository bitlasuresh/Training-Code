<%= form_with scope: :login, url:logins_path, local: true do |form| %>
	<form class="form-inline">
  		<div class="form-group">
			<div>
		    	<%= form.label :username %>
		    	<%= form.text_field :name %>
		  	</div>
		  	<div>
		  		<%= form.label:password %>
		  		<%= form.password_field :password %>
		  	</div>
		  	<br>
		  	<div>
		    	<%= form.submit class: "btn btn-success" %>
		  	</div>
	  	</div>
  	</form>
<%end %>